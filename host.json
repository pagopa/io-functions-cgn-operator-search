{  
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5,
        "includedTypes": "PageView;Trace;Dependency",
        "excludedTypes": "Exception;Event;CustomEvent;Request"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    }
  }
}
